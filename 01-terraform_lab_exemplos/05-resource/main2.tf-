provider "aws" {
  region = "sa-east-1"
}

resource "aws_instance" "web" {
  for_each = toset(["Vini 1","Vini 2", "Vini 3"])

  subnet_id     = "subnet-048155f5678ed3564"
  ami= "ami-07c267c1d2395046a"
  instance_type = "t2.micro"
  root_block_device {
    encrypted = true
    volume_size = 8
  }
  tags = {
    Name = "ec2-vinicius-${each.key}"
  }
}